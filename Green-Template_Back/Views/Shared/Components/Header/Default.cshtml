@model HomeVM  
<header>
      <section id="contact">
          <div class="container">
              <div class="row">
                      <div class="col-lg-6">
                         <div class="email-phone">
                             <ul>
                                 <li><i class="fa-solid fa-envelope"></i>
                                  <a href="#">@Model.Settings.Email</a>
                                    </li>
                                    <li> <i class="fa-solid fa-mobile-screen-button"></i>
                                        <a href="#">@Model.Settings.Phone</a>
                                          </li>
                             </ul>
                         </div>
                      </div>

                      <div class="col-lg-6">
                          <div class="social-media">
                              <ul>
                                  @foreach (var item in Model.SocialMedias)
                                 {
                                      <li><a href="#"><i class="@item.Icon"></i></a></li>
                                 }
                              </ul>
                          </div>
                      </div>
              </div>
          </div>
      </section>
      <section id="navbar">
          <div class="container">
              <div class="row">
                  <div class="col-lg-4">
                      <div class="logo">
                          <h2>
                              <a href="#">@Model.Settings.HeaderLogo</a>
                          </h2>
                      </div>
                  </div>

                  <div class="col-lg-8">
                      <div class="nav">
                          <ul>
                              <li><a href="#">Home</a></li>
                              <li><a href="#">About</a></li>
                              <li><a href="#">Services</a></li>
                              <li><a href="#">Portfolio</a></li>
                              <li><a href="#">Team</a></li>
                              <li><a href="#">Drop Down<i class="fa-solid fa-angle-down"></i></a></li>
                              <li><a href="#">Contact</a></li>
                              <li><button type="button" class="btn btn-outline-success button">Get Started</button></li>
                          </ul>
                      </div>
                  </div>
              </div>
          </div>
      </section>
    </header>